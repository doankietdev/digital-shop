    "totp-generator": "^1.0.0",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.10",
    "@babel/core": "^7.22.10",
    "@babel/eslint-parser": "^7.22.10",
    "@babel/node": "^7.22.10",
    "@babel/plugin-transform-runtime": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.47.0",
    "morgan": "^1.10.0",
    "nodemon": "^3.0.1"
  }
}
